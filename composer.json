{
    "name": "idci/configuration-validator-bundle",
    "description": "Symfony ConfigurationValidatorBundle",
    "keywords": ["symfony2", "configuration", "validator"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Brahim Boukoufallah",
        "email": "brahim.boukoufallah@idci-consulting.fr",
        "homepage": "http://www.idci-consulting.fr"
    }],
    "require": {
        "php"                          : ">=5.3.0",
        "symfony/dependency-injection" : "^3.4",
        "symfony/config"               : "^3.4",
        "symfony/yaml"                 : "^3.4"
    },
    "suggest": {
        "symfony/framework-bundle" : "To use this lib as a full Symfony Bundle"
    },
    "require-dev": {
        "symfony/debug"   : "~2.3 || ~3.0",
        "phpunit/phpunit" : ">=3.7.0"
    },
    "autoload": {
        "psr-0": { "IDCI\\Bundle\\ConfigurationValidatorBundle\\": "" }
    },
    "target-dir": "IDCI/Bundle/ConfigurationValidatorBundle"
}
